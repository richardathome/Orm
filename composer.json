{
  "name": "richbuilds/orm",
  "description": "A wireless ORM for PHP",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Richard Allsebrook",
      "email": "richardathome@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "ext-bcmath": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "10.2.x-dev",
    "phpstan/phpstan": "1.11.x-dev",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Richbuilds\\Orm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Richbuilds\\Orm\\Tests\\": "tests/"
    }
  }
}
